{
  "compilerOptions": {
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "jsx": "react",
    "esModuleInterop": true,
    "baseUrl": "src",
    "paths": {
      "*": ["*"]
    },
    "moduleResolution":"node"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}